{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.2.1",
  "description": "__MSG_extDesc__",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
